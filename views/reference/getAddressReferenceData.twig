{% extends "./layout.twig" %}
{% block title %}{{ title }}{% endblock %}
{% block head %}
    {{ parent() }}
{% endblock %}
{% block content %}
    <ol class="breadcrumb">
        <li><a href="/">Home</a></li>
        <li><a href="/reference">Reference Services</a></li>
    </ol>
    <div class="container">
        <h1>{{ Header }}</h1>
        <div class="well">
            <form class="formentry" role="form" action="/reference/getAddressReferenceData" method="post">
                <div class="form-group">
                    <label for="LastEditDate">Last Edit Date</label>
                    <input type="date" class="form-control" name="LastEditDate" value="{{ LastEditDate }}" >
                </div>

                <div class="form-group">
                    <label for="DisplayOnExternalSystem">Display On External System</label>
                    <input type="checkbox" class="form-control" name="DisplayOnExternalSystem" value="{{ DisplayOnExternalSystem }}" >
                </div>

                <button type="submit" class="btn btn-default">Submit</button>
            </form>
        </div>
    </div>
{% endblock %}